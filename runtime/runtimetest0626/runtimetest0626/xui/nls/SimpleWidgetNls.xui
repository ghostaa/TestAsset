    <rule name="IsFocusableClick">
      <events>
        <event widget="btnIsFocusable" event="onClick"/>
      </events>
      <condition>
        <value type="widgetFunction" widget="btnForTest">
          <function name="isFocusable"/>
        </value>
      </condition>
      <trueActions stopEventPagination="false">
        <action type="widgetProperty" widget="lblInfo" property="text">
          <value type="constant" constant="'Button isFocusable return TRUE'"/>
        </action>
      </trueActions>
      <falseActions stopEventPagination="false">
        <action type="widgetProperty" widget="lblInfo" property="text">
          <value type="constant" constant="'Button isFocusable return FALSE'"/>
        </action>
      </falseActions>
    </rule>
