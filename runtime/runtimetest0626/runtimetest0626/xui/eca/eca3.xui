  <rules>
    <rule name="preSubmitDos">
      <events>
        <event widget="btnSubmit" event="onClick"/>
      </events>
      <condition>
        <value type="expression">
          <leftValue type="widgetProperty" widget="rtxtSignature" property="value"/>
          <operator name="!="/>
          <rightValue type="constant" constant="''"/>
        </value>
      </condition>
      <trueActions stopEventPagination="false">
        <action type="widgetProperty" widget="rtxtSignatureCopy" property="value">
          <value type="widgetProperty" widget="rtxtSignature" property="value"/>
        </action>
        <action type="widgetProperty" widget="btnSubmit" property="text">
          <value type="constant" constant="'Submitting...'"/>
        </action>
        <action type="widgetProperty" widget="rtxtSignature" property="readOnly">
          <value type="constant" constant="true"/>
        </action>
        <action type="widgetProperty" widget="rtxtInfo" property="value">
          <value type="constant" constant="'Submitting forms to server...'"/>
        </action>
      </trueActions>
      <falseActions stopEventPagination="false">
        <action type="widgetProperty" widget="rtxtInfo" property="value">
          <value type="constant" constant="'You must input a signature to signature box before submitting to server.'"/>
        </action>
      </falseActions>
    </rule>
    <rule name="doAfterReset">
      <events>
        <event widget="btnReset" event="onClick"/>
      </events>
      <trueActions stopEventPagination="false">
        <action type="widgetProperty" widget="rtxtSignature" property="readOnly">
          <value type="constant" constant="false"/>
        </action>
        <action type="widgetProperty" widget="rtxtInfo" property="value">
          <value type="constant" constant="'Signature boxes can be edited now!'"/>
        </action>
        <action type="widgetProperty" widget="rtxtSignature" property="visibility">
          <value type="constant" constant="'visible'"/>
        </action>
        <action type="widgetProperty" widget="rtxtSignatureCopy" property="visibility">
          <value type="constant" constant="'visible'"/>
        </action>
      </trueActions>
      <falseActions stopEventPagination="false">
        <action type="widgetProperty" widget="rtxtInfo" property="value">
          <value type="constant" constant="'All signature boxes are editable!'"/>
        </action>
      </falseActions>
    </rule>
