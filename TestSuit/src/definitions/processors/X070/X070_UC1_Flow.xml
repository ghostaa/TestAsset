<?xml version="1.0" encoding="UTF-8"?>
<X070_UC1_Flow>


	<htmlProcessor context="X070_UC1_Ctx" id="X070_UC1_Flow">
		<htmlState id="initial" type="initial">
			<transitions>
				<htmlTransition id="initial.start" targetState="X070_InitDataOp"/>
			</transitions>
		</htmlState>

		<htmlState typeIdInfo="OK" id="OK" type="final"/>

		<operationState typeIdInfo="X070_InitDataOp" id="X070_InitDataOp" outputMapFmt="X070_InitDataOp_OutputFmt" type="operation">
			<transitions>
				<htmlTransition id="X070_InitDataOp.opEvt" outputMapFmt="X070_InitDataOp_OutputFmt" targetState="X070X070_UC1"/>
			</transitions>
		</operationState>

		<htmlState type="page" typeIdInfo="X070/X070_UC1.jsp" id="X070X070_UC1">
			<transitions>
				<htmlTransition id="X070X070_UC1.viewEvt" targetState="OK"/>
			</transitions>
		</htmlState>
	</htmlProcessor>

	<context id="X070_UC1_Ctx" type="op">
		<refKColl refId="X070_UC1_Record"/>
	</context>

	<kColl id="X070_UC1_Record">
		<refData refId="cashAcctList"/>
	</kColl>

	<kColl id="cashAcct">
		<data id="name" refType="String"/>
		<data id="surname" refType="String"/>
		<data id="balance" refType="Double"/>
		<field id="branch_location"/>
		<field id="branch_id"/>
		<data id="X070_UC1_Data" refType="Double"/>
	</kColl>

	<!-- Data for transfer history -->
	<iColl id="cashAcctList">
		<refData refId="cashAcct" lazy="false"/>
	</iColl>

	<fmtDef id="X070_InitDataOp_OutputFmt">
		<mapperConverterExpression>
			<map from="cashAcctList" to="cashAcctList" append="false"/>
		</mapperConverterExpression>
	</fmtDef>

</X070_UC1_Flow>
